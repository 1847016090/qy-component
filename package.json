{
  "name": "qy-component",
  "version": "1.0.4",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/umijs/qy-component.git",
    "branch": "main"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd": "^4.21.4",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.5",
    "dumi": "^1.1.0",
    "father-build": "^1.19.1",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.7.1",
    "yorkie": "^2.0.0"
  },
  "description": "A doc template can assist you to develop libraries & write docs.",
  "bugs": {
    "url": "https://github.com/umijs/qy-component/issues"
  },
  "homepage": "https://github.com/umijs/qy-component#readme",
  "directories": {
    "doc": "docs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
